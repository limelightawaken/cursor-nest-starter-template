# Application
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/template?schema=public"

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-at-least-32-characters-long
JWT_EXPIRES_IN=24h

# CORS Configuration
CORS_ENABLED=true
CORS_ORIGIN=*

# Swagger Documentation
SWAGGER_ENABLED=true
SWAGGER_TITLE=template api
SWAGGER_DESCRIPTION=API documentation for template backend services
SWAGGER_VERSION=1.0
SWAGGER_PATH=api

# Rate Limiting
THROTTLE_TTL=60000
THROTTLE_LIMIT=10
